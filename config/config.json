{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "firstwebpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/firstwebpart/FirstwebpartWebPart.js",
          "manifest": "./src/webparts/firstwebpart/FirstwebpartWebPart.manifest.json"
        }
      ]
    },
    "property-pane-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/propertyPaneWebpart/PropertyPaneWebpartWebPart.js",
          "manifest": "./src/webparts/propertyPaneWebpart/PropertyPaneWebpartWebPart.manifest.json"
        }
      ]
    },
    "spfx-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxForm/SpfxFormWebPart.js",
          "manifest": "./src/webparts/spfxForm/SpfxFormWebPart.manifest.json"
        }
      ]
    },
    "formik-validation-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/formikValidationForm/FormikValidationFormWebPart.js",
          "manifest": "./src/webparts/formikValidationForm/FormikValidationFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FirstwebpartWebPartStrings": "lib/webparts/firstwebpart/loc/{locale}.js",
    "PropertyPaneWebpartWebPartStrings": "lib/webparts/propertyPaneWebpart/loc/{locale}.js",
    "SpfxFormWebPartStrings": "lib/webparts/spfxForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "FormikValidationFormWebPartStrings": "lib/webparts/formikValidationForm/loc/{locale}.js"
  }
}
